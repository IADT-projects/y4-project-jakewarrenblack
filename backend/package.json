{
  "name": "raid-backend",
  "version": "1.0.0",
  "description": "Live webcam streaming and S3 upload interface for RAID frontend",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon server.js ",
    "start": "concurrently \"nodemon server.js\" \"(cd livecam && node index.js)\"",
    "install_arm64": "build-opencv --version 4.5.4 --flag=\"-DCMAKE_SYSTEM_PROCESSOR=arm64 -DCMAKE_OSX_ARCHITECTURES=arm64\" build",
    "install_4.5.5_cuda": "build-opencv --version 4.5.5 --flags=\"-DWITH_CUDA=ON -DWITH_CUDNN=ON -DOPENCV_DNN_CUDA=ON -DCUDA_FAST_MATH=ON\" build",
    "do-install": "build-opencv build"
  },
  "author": "Jake Black",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.223.0",
    "@u4/opencv4nodejs": "^6.5.0",
    "axios": "^1.3.4",
    "base64-arraybuffer": "^1.0.2",
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "console": "^0.7.2",
    "cors": "^2.8.5",
    "dicer": "^0.3.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "fs": "^0.0.1-security",
    "jimp": "^0.22.7",
    "mri": "^1.2.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "picocolors": "^1.0.0",
    "progress": "^2.0.3",
    "qrcode": "^1.5.1",
    "qrcode-reader": "^1.0.4",
    "raw-body": "^2.5.1",
    "sharp": "^0.31.3",
    "socket.io": "^4.6.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  }
}
